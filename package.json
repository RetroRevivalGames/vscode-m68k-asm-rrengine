{
    "name": "m68k-asm-gnu",
    "displayName": "M68000 Assembly (GNU AS)",
    "description": "m68k asm (gnu as) syntax highlighting for VS code",
    "version": "1.1.0",
    "publisher": "StanH",
    "repository": "https://github.com/StanHash/vscode-m68k-asm-gnu",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "m68k-gnu",
            "aliases": ["M68000 (GNU AS)", "m68k-gnu"],
            "extensions": [".s",".asm"],
            "configuration": "./configs/m68k-gnu-configuration.json"
        }],
        "grammars": [{
            "language": "m68k-gnu",
            "scopeName": "source.m68k.gnu",
            "path": "./syntaxes/m68k-gnu.tmLanguage.json"
        }]
    }
}